<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.gunSelf.modular.system.dao.ProductMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.gunSelf.modular.system.model.Product">
        <id column="productNo" property="productNo" />
        <result column="productName" property="productName" />
        <result column="productPlace" property="productPlace" />
        <result column="productSpecs" property="productSpecs" />
        <result column="productPrice" property="productPrice" />
        <result column="productYear" property="productYear" />
        <result column="stockNumber" property="stockNumber" />
        <result column="categoryNo" property="categoryNo" />
        <result column="isDelete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        productNo, productName, productPlace, productSpecs, productPrice, productYear, stockNumber, categoryNo, isDelete
    </sql>

</mapper>
